<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="page.main.title" destination="page.top" after="breadcrumbs" />
        <referenceBlock name="category.product.type.details.renderers" remove="true"/>
        <referenceBlock name="product.info.addtocart" remove="true" />
        <referenceBlock name="category.product.addto" remove="true"/>
        <referenceContainer name="content">
            <block class="Conceptive\Commerce\Block\Index" name="category.page.custom" template="Conceptive_Commerce::category_page.phtml" />
        </referenceContainer>
    </body>
</page>